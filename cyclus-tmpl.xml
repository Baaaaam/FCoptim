<simulation>

  <schematype>flat</schematype>
  <control>
    <simhandle>{{.Handle}}</simhandle> 
    <duration>2400</duration>
    <startmonth>1</startmonth>
    <startyear>2015</startyear>
    <decay>never</decay>
    <explicit_inventory>true</explicit_inventory>
  </control>

  <archetypes>
    <spec> <lib>cycamore</lib> <name>Source</name></spec>
    <spec> <lib>cycamore</lib> <name>GroupInst</name></spec>
    <spec> <lib>cycamore</lib> <name>DeployInst</name></spec>
    <spec> <lib>cycamore</lib> <name>Sink</name></spec>
    <spec> <lib>cycamore</lib> <name>Enrichment</name> </spec>
    <spec> <lib>cycamore</lib> <name>Reactor</name> </spec>
    <spec> <lib>cycamore</lib> <name>Storage</name> </spec>
    <spec> <lib>cycamore</lib> <name>FuelFab</name> </spec>
    <spec> <lib>cycamore</lib> <name>Separations</name> </spec>
    <spec> <lib>cycamore</lib> <name>Mixer</name> </spec>
  </archetypes>


  <prototype>
    <name>FBR_driver_fabrication</name>
    <lifetime>960</lifetime>
    <config>
      <Mixer>

        <in_streams>
          <stream>
            <info>
              <mixing_ratio>8.13497471866829081e-01</mixing_ratio>
              <buf_size>5300</buf_size>
            </info>
            <commodities>
              <item>
                <commodity>E1_stored</commodity>
                <pref>1</pref>
              </item>
            </commodities>
          </stream>
          <stream>
            <info>
              <mixing_ratio>1.86506111596205865e-01</mixing_ratio>
              <buf_size>1300</buf_size>
            </info>
            <commodities>
              <item>
                <commodity>E2_stored</commodity>
                <pref>1</pref>
              </item>
            </commodities>
          </stream>
        </in_streams>

        <throughput>6500</throughput>
        <out_commod>FBR_driver_MOX_new</out_commod>
        <out_buf_size>6500</out_buf_size>

      </Mixer>
    </config>
  </prototype>


  <prototype>
    <name>FBR_blanket_fabrication</name>
